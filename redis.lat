funcion db_asignar(llave, valor)
	url = peticion(def.tipo->asignar.llave."/".codificar(valor))
	res = json_decodificar(url)
fin

funcion db_obtener(llave, valor)
	url = peticion(def.tipo->obtener.llave)
	res = json_decodificar(url)
	si ! res->GET
		retorno ""
	sino
		retorno res->GET
	fin
fin

